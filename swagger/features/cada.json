      "/cada/file": {
      "get": {
        "tags": ["cadaFile"],
        "summary": "List all Cada files",
        "operationId": "listAllCadaFiles",
        "description": "Retrieves a paginated, sortable list of all Cada files, optionally filtered by a 'search' query string.",
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "schema": { "type": "integer" },
            "description": "Page number (default=1)"
          },
          {
            "name": "pageSize",
            "in": "query",
            "schema": { "type": "integer" },
            "description": "Number of results per page (default=20)"
          },
          {
            "name": "sortOrder",
            "in": "query",
            "schema": {
              "type": "string",
              "enum": ["asc", "desc"]
            },
            "description": "Sorting order, default=desc"
          },
          {
            "name": "search",
            "in": "query",
            "schema": { "type": "string" },
            "description": "Partial search string for matching the 'path' field."
          }
        ],
        "responses": {
          "200": { "description": "A list of Cada files." },
          "500": { "description": "Internal Server Error" }
        }
      },
      "post": {
        "tags": ["Cada File"],
        "summary": "Create Bulk Cada Files",
        "operationId": "createBulkCadaFiles",
        "description": "Saves multiple Cada files to the database.",
        "requestBody": {
          "required": true,
          "description": "Array of Cada file objects to insert.",
          "content": {
            "application/json": {
              "schema": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "path": { "type": "string" },
                    "type": { "type": "string" },
                    "ext": { "type": "string" },
                    "info": { "type": "string" }
                  },
                  "required": ["path", "type", "ext"]
                },
                "example": [
                  {
                    "path": "/uploads/sample1.pdf",
                    "type": "document",
                    "ext": "pdf",
                    "info": "Sample document file."
                  },
                  {
                    "path": "/uploads/sample2.jpg",
                    "type": "image",
                    "ext": "jpg",
                    "info": "Sample image file."
                  }
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "An array of upserted or existing files with their IDs."
          },
          "500": { "description": "Internal Server Error" }
        }
      },
      "delete": {
        "tags": ["Cada File"],
        "summary": "Delete Bulk Cada Files",
        "operationId": "deleteBulkCadaFiles",
        "description": "Deletes multiple Cada files by their IDs.",
        "requestBody": {
          "required": true,
          "description": "JSON object with file_ids array.",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "file_ids": {
                    "type": "array",
                    "items": { "type": "integer" }
                  }
                },
                "example": {
                  "file_ids": [1, 2, 3]
                }
              }
            }
          }
        },
        "responses": {
          "200": { "description": "Number of files deleted or success message." },
          "404": { "description": "No matching files found to delete." },
          "500": { "description": "Internal Server Error" }
        }
      }
