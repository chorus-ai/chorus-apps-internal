        "description": "Searches for Cada files based on path substring, with pagination & sorting.",
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "schema": { "type": "integer" },
            "description": "Page number, default=1"
          },
          {
            "name": "pageSize",
            "in": "query",
            "schema": { "type": "integer" },
            "description": "Number of results per page, default=20"
          },
          {
            "name": "sortOrder",
            "in": "query",
            "schema": {
              "type": "string",
              "enum": ["asc", "desc"]
            },
            "description": "Sorting order, default=desc"
          }
        ],
        "requestBody": {
          "required": true,
          "description": "JSON object containing 'path' property to search partial matches.",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "path": { "type": "string" }
                },
                "example": {
                  "path": "/uploads/sample"
                }
              }
            }
          }
        },
        "responses": {
          "200": { "description": "Returns an array of Cada files matching the path substring." },
          "404": { "description": "No files found matching path." },
          "500": { "description": "Internal Server Error" }
        }
      }
    }
