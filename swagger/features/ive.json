  "components": {
    "requestBodies": {
      "iveComponent": {
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string",
                  "default": "MyAlarm"
                },
                "title": {
                  "type": "string",
                  "default": "Neuro Alarm"
                },
                "description": {
                  "type": "string",
                  "default": "The alarm component"
                },
                "attributes": {
                  "type": "string",
                  "default": null
                },
                "componentType": {
                  "type": "string",
                  "description": "Component type",
                  "enum": [
                    "alarm",
                    "waveform",
                    "vitals",
                    "demographic"
                  ]
                }
              }
            }
          }
        }
      }
    }
  }
